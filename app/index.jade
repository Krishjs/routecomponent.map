doctype html
html(lang="en")
  head
    meta(charset="utf-8")
    meta(name="viewport" content="width=device-width, initial-scale=1.0 ,user-scalable=no")
    title Google Maps
    style().
        html, body {
            height: 100%;
            margin: 0;
            padding: 0;
        }
        .map-wrap {
            height: 85%;
        }

        .search-box {
            height: 15%;
            padding-left: 22px;
            min-height: 80px;
        }

        .FL {
            float: left;
        }

        .float-panel {
            position: absolute;
            top: 250px;
            height:200px;
            width:200px;
            left: 2%;
            z-index: 5;
            padding: 5px;
            border: 1px solid #999;
            text-align: center;
            font-family: 'Roboto','sans-serif';
            line-height: 30px;
            padding-left: 10px;            
        }

        .kjs-label {
        }

        .kjs-text {
        }

        .search-box fieldset {
            height: 50%;
            min-height: 50px;
        }

        .search-box legend {
            font-size: x-large;
            font-weight: bold;
        }
  body
    #searchbox.search-box
     fieldset
       legend Sea Way
       label( class="kjs-label FL" for="from") Show Route:
       input(class="kjs-checkbox FL" type="checkbox" id="showRoute" onchange="kjsmap.RoutesView(this.checked)")
       label(class="kjs-label FL" for="from") From:
       input(class="kjs-text FL" type="text" id="from" onchange="kjsmap.Form.FromPort.OnChange(this, event)")
       label(class="kjs-label FL" for="To") To:
       input(class="kjs-text FL" type="text" id="to" onchange="kjsmap.Form.ToPort.OnChange(this, event)")
       input(class="kjs-btn FL" type="button" id="search" onclick="kjsmap.Form.Search(this, event)" value="Search")
       label(class="kjs-label FL" for="from") Change Radius:
       input(class="kjs-range FL" type="range" id="cricle" min="1" max='100' value="50" step="1" oninput="kjsmap.Form.RangeChanged(this,event)" onchange="kjsmap.Form.RangeChanged(this,event)")
    #map.map-wrap
    script(src="js/map.js?v=5122")
    script(src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC5YpNj7AK2gZ-iY5Fvz9vkhy2Ml1IwAmM&libraries=places&callback=initMap&language=en-US&region=in")
    script(src="https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore.js")
            